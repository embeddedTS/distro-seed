CONFIG_DS_KERNEL_PROVIDER_GIT_VERSION="v5.10.170.4-ts"
CONFIG_DS_KERNEL_DEFCONFIG="tsimx6_minimal_defconfig"
CONFIG_DS_KERNEL_INSTALL_DEVICETREE_FILESYSTEM="imx6q-ts7970 imx6dl-ts7970"
# CONFIG_DS_KERNEL_INSTALL_ZIMAGE_FILESYSTEM is not set
CONFIG_DS_KERNEL_INSTALL_UIMAGE_FILESYSTEM=y
CONFIG_DS_KERNEL_INSTALL_UIMAGE_LOADADDR=0x10008000
# end of Kernel

#
# Packages
#
# CONFIG_DS_PACKAGE_DISTROBOOT_SCRIPTS is not set
CONFIG_DS_PACKAGE_LEGACY_BOOTSCRIPT=y
CONFIG_DS_PACKAGE_LEGACY_BOOTSCRIPT_CMDLINE="console=ttymxc0,115200 init=/sbin/init loglevel=4"
# CONFIG_DS_PACKAGE_LEGACY_BOOTSCRIPT_INSTALL_SOURCE is not set
CONFIG_DS_PACKAGE_RESIZE=y

#
# Hardware Specific Packages
#
# CONFIG_DS_PACKAGE_TS4900_UTILS is not set
CONFIG_DS_PACKAGE_TS4900_FPGA=y
# end of Hardware Specific Packages
# end of Packages

#
# Image Configuration
#

#
# Required Settings
#
CONFIG_DS_LOCALES="en_US.UTF-8"
CONFIG_DS_PURGE_LOCALES=y
CONFIG_DS_HOSTNAME=""
CONFIG_DS_TIMEZONE="America/Phoenix"
CONFIG_DS_FSTAB_GEN=y
# CONFIG_DS_FSTAB_FILE is not set

#
# geneated fstab options
#
CONFIG_DS_FSTAB_GEN_ROOT_DEVICE="/dev/root"
CONFIG_DS_FSTAB_GEN_ROOT_FS="ext4"
CONFIG_DS_FSTAB_GEN_ROOT_OPTIONS="defaults"
# end of geneated fstab options

