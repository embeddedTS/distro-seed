CONFIG_DS_ARCH_ARMHF=y
# CONFIG_DS_ARCH_ARMEL is not set
# end of Target Options

#
# Distribution
#
# CONFIG_DS_DISTRO_DEBIAN_11 is not set
# CONFIG_DS_DISTRO_DEBIAN_12 is not set
# CONFIG_DS_DISTRO_UBUNTU_22_04 is not set
